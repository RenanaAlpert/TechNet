<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MainActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="203dp"
        android:layout_height="46dp"
        android:text="@string/report_malfunction"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#040404"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="147dp"
        android:layout_height="29dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="5dp"
        android:text="@string/school_detailes"
        android:textAlignment="viewEnd"
        android:textColor="#0C0C0C"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:background="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="5dp"
        android:text="@string/school_name"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="95dp"
        android:ems="10"
        android:hint="@string/hint_edt_school_name"
        android:inputType="textPersonName"
        android:textColor="#736C6C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="158dp"
        android:layout_marginEnd="5dp"
        android:text="@string/town"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="145dp"
        android:ems="10"
        android:hint="@string/hint_edt_town"
        android:inputType="textPersonName"
        android:textColor="#736C6C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="5dp"
        android:text="@string/street"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="195dp"
        android:ems="10"
        android:hint="@string/hint_edt_street"
        android:inputType="textPersonName"
        android:textColor="#736C6C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="258dp"
        android:layout_marginEnd="5dp"
        android:text="@string/number"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="253dp"
        android:layout_marginTop="245dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="147dp"
        android:layout_height="29dp"
        android:layout_marginTop="305dp"
        android:layout_marginEnd="5dp"
        android:text="@string/malfunction_detailes_headline"
        android:textAlignment="viewEnd"
        android:textColor="#0C0C0C"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:background="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.456"
        tools:layout_editor_absoluteX="-7dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="357dp"
        android:layout_marginEnd="5dp"
        android:text="@string/device_type"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="344dp"
        android:ems="10"
        android:hint="@string/hint_edt_device_type"
        android:inputType="textPersonName"
        android:textColor="#736C6C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="122dp"
        android:layout_height="31dp"
        android:layout_marginTop="407dp"
        android:layout_marginEnd="5dp"
        android:text="@string/model"
        android:textColor="#090909"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="291dp"
        android:layout_height="48dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="394dp"
        android:ems="10"
        android:hint="@string/hint_edt_model"
        android:inputType="textPersonName"
        android:textColor="#736C6C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>